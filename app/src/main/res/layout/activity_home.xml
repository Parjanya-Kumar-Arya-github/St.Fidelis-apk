<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".home">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="0dp"
        android:layout_height="197dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.614"
        app:srcCompat="@drawable/rect" />

    <ImageView
        android:id="@+id/stars"
        android:layout_width="1666dp"
        android:layout_height="1666dp"
        android:rotation="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.337"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.336"
        app:srcCompat="@drawable/stars" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="267dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rect" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="92dp"
        android:layout_margin="0dp"
        android:contentDescription="TODO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/ic_wave" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="254dp"
        android:layout_height="84dp"
        android:rotation="0"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.611"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.185"
        app:srcCompat="@drawable/parallelogrm" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="0dp"
        android:layout_height="101dp"
        android:rotation="0"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:layout_constraintVertical_bias="0.096"
        app:srcCompat="@drawable/parallelogrm" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="219dp"
        android:layout_height="53dp"
        android:fontFamily="@font/rammetto_one"
        android:gravity="center"
        android:rotation="0"
        android:text="St. Fidelis"
        android:textColor="#00cba9"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="346dp"
        android:layout_height="103dp"
        android:fontFamily="@font/rammetto_one"
        android:gravity="center"
        android:rotation="0"
        android:text="Parjanya Kumar Arya"
        android:textColor="#00cba9"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="123dp"
        android:layout_height="113dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/logo" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/spaceship"
        android:layout_width="201dp"
        android:layout_height="198dp"
        android:rotation="45"
        android:src="@drawable/spaceship"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/about_me"
        android:layout_width="125dp"
        android:layout_height="62dp"
        android:layout_marginStart="52dp"
        app:layout_constraintBottom_toTopOf="@+id/spaceship"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.599"
        app:srcCompat="@drawable/aboutme" />

    <ImageView
        android:id="@+id/clock_gallery"
        android:layout_width="125dp"
        android:layout_height="62dp"
        app:layout_constraintBottom_toTopOf="@+id/spaceship"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/about_me"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.599"
        app:srcCompat="@drawable/aboutme" />

    <TextView
        android:id="@+id/about_text"
        android:layout_width="74dp"
        android:layout_height="66dp"
        android:fontFamily="@font/racing_sans_one"
        android:gravity="center"
        android:text="@string/about_me"
        android:textColor="#FFFFFF"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="@+id/about_me"
        app:layout_constraintEnd_toEndOf="@+id/about_me"
        app:layout_constraintStart_toStartOf="@+id/about_me"
        app:layout_constraintTop_toTopOf="@+id/about_me" />

    <TextView
        android:id="@+id/clock_gallery_text"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/racing_sans_one"
        android:gravity="center"
        android:text="@string/clock_gallery"
        android:textColor="#FFFFFF"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="@+id/clock_gallery"
        app:layout_constraintEnd_toEndOf="@+id/clock_gallery"
        app:layout_constraintStart_toStartOf="@+id/clock_gallery"
        app:layout_constraintTop_toTopOf="@+id/clock_gallery" />

    <ImageView
        android:id="@+id/splash"
        android:layout_width="1dp"
        android:layout_height="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/splash_home" />

    <TextClock
        android:id="@+id/time_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/orbitron"
        android:format12Hour="hh:mm:ss"
        android:format24Hour="hh:mm:ss"
        android:shadowColor="#00cba9"
        android:shadowDx="0.0"
        android:shadowDy="0.0"
        android:shadowRadius="8"
        android:text="00:00:00"
        android:textColor="#00cba9"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/about_me"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextClock
        android:id="@+id/am_pm_home"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="3dp"
        android:fontFamily="@font/orbitron_black"
        android:format12Hour="a"
        android:format24Hour="a"
        android:shadowColor="#00cba9"
        android:gravity="center"
        android:shadowDx="0.0"
        android:shadowDy="0.0"
        android:shadowRadius="8"
        android:text="00:00:00"
        android:textColor="#00cba9"
        app:layout_constraintBottom_toBottomOf="@+id/time_home"
        app:layout_constraintStart_toEndOf="@+id/time_home"
        app:layout_constraintTop_toTopOf="@+id/time_home" />


</androidx.constraintlayout.widget.ConstraintLayout>
